{
  "name": "helson",
  "version": "0.0.0",
  "description": "JavaScript Object Schema Checker",
  "main": "dist/helson.js",
  "scripts": {
    "test": "npm run generate && mv helson.js src/helson.js && mocha",
    "test:debug": "npm run generate && mv helson.js src/helson.js && mocha --inspect-brk=0.0.0.0:9229",
    "start:debug": "node --inspect-brk=0.0.0.0:9229 src/index",
    "generate": "jison src/helson.y src/helson.lex",
    "lint": "eslint src/"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Object",
    "schema",
    "checker",
    "test",
    "gatekeeper",
    "compiler"
  ],
  "author": "Akash Goswami",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "jison": "^0.4.18",
    "mocha": "^6.2.0"
  }
}
